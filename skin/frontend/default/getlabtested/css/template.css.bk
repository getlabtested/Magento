/* CSS Document */

/* --- general --- */

html, body {
	margin: 0;
	padding: 0;
	background: #0c6e8e;
	font-size: 13px;
	color: #646262;
	font-family: Arial, Helvetica, sans-serif;	
	overflow-x: hidden;
}

#all, .all {
	position: relative;
	width: 960px;
	margin-left: 50%;
	left: -480px;	
}

a {
	text-decoration: none;
}

a img {
	border: none;
}

a.read_more {
	font-weight: bold;
	color: #028afb;
}

*:focus {
	outline: none;
}

div.clear {
	width: 100%;
	height: 0px;
	clear: both;	
	overflow: hidden;
}

div.sep5 {
	height: 5px;
}

div.sep10 {
	height: 10px;
}

div.sep15 {
	height: 15px;
}

div.sep20 {
	height: 20px;	
}

.orange {
	color: #e77640;
	font-weight: bold;
}

.blue {
	font-weight: bold;
	color: #0c6e8e;
}	

.blue_light {
		font-weight: bold;
		color: #028afb;
}

.bg_white {
	background: #fff;
	overflow: hidden;
}

.bg_blue {
	background: #0c6e8e;	
	overflow: hidden;
}

.bg_grey {
	background: #f2f2f2;
	overflow: hidden;
}

.bg_light_blue {
	background: #ecf3f6;	
}

.bg_grey_round {
	background: #f3f3f3;
	padding: 15px;
	-moz-border-radius: 10px;
 	border-radius: 10px;	
}

h1 {
	font-size: 28px;
	font-family: Tahoma, Geneva, sans-serif;
	color: #023a4c;
}

h2 {
	font-family: Tahoma, Geneva, sans-serif;
	font-size: 19px;
	font-weight: normal;
	color: #0c6e8e;
}

h3, h4 { 
	font-family: Tahoma, Geneva, sans-serif;
	font-size: 15px;
	color: #e77640;	
}

h5, h6 {
	font-size: 13px;
	font-weight: bold;
	color: #717171;	
}

h1 a, h2 a, h3 a, h4 a {
	color: inherit;
}

.btn_orange {
	background: #e97640;
	color: #fff;
	font-weight: bold;
	font-family: Tahoma, Geneva, sans-serif;
	text-align: center;
	display: block;
	width: auto;
	padding: 10px 20px;
	height: 20px;
	text-transform: uppercase;
	-moz-border-radius: 10px;
 	border-radius: 10px;
}

.btn_orange:hover {
	background: #fe8e59;
}

.btn_orange_reflection {
	display: block;
	width: 224px;
	height: 20px;
	padding: 13px 0px 12px 0px;
	font-size: 17px;
	font-weight: bold;
	color: #fff;
	text-align: center;
	text-transform: uppercase;
	background: url(../images/btn_orange_reflection.png) center center no-repeat;
	-moz-border-radius: 10px;
 	border-radius: 10px;
}

input.btn_orange_reflection {
	height: 45px;	
}

.btn_orange_32_reflection {
	display: block;
	width: auto;
	height: 18px;
	padding: 7px 0px;
	font-size: 13px;
	font-weight: bold;
	color: #fff;
	text-align: center;
	text-transform: uppercase;
	background: url(../images/btn_orange_32_reflection.png) center center no-repeat;
	-moz-border-radius: 5px;
 	border-radius: 5px;
}

input.btn_orange_32_reflection {
	height: 32px;
	border: none;
	padding: 0px 15px;
	cursor: pointer;
	width: auto;
}

.btn_orange_25_reflection {
	display: block;
	width: auto;
	height: 13px;
	padding: 6px 10px;
	font-size: 10px;
	font-weight: bold;
	color: #fff;
	text-align: center;
	text-transform: uppercase;
	background: url(../images/btn_orange_25_reflection.png) center center no-repeat;
	-moz-border-radius: 5px;
 	border-radius: 5px;	
}

.btn_orange_shadow {
	display: block;
	width: 340px;
	height: 67px;
	padding-top: 30px;
	background: url(../images/btn_orange_shadow.png) center center no-repeat;
	text-align: center;
	font-size: 29px;
	font-weight: bold;
	color: #fff;
	text-shadow: #ae3a04 2px 2px 1px;	
}

.btn_blue_reflection {
	-moz-border-radius: 5px;
 	border-radius: 5px;
	font-size: 15px;
	font-weight: bold;
	color: #fff;
	text-transform: uppercase;
	width: 156px;
	height: 23px;
	display: block;
	padding-top: 7px;
	text-align: center;
	border: none;
	background: url(../images/btn_blue_reflection.png) center center no-repeat;
}

input.submit {
	border: none;
	padding: 0px 15px;
	cursor: pointer;
	width: auto;
}

.box_dark {
	background: #0c3f4f;
	border: 3px solid #1b83a5;
	-moz-border-radius: 15px;
 	border-radius: 15px;
	color: #bfe2ff;
	padding: 20px;
}

.box_blue_header, .box_orange_header {
	width: 233px;
	border: 1px solid #c1c1c1;
	-moz-border-radius: 5px;
 	border-radius: 5px;
	margin-top: 43px;
}

.box_blue_header .header {
	background: #0b6d91;
	padding: 12px;
	text-align: center;	
	-moz-border-radius: 5px 5px 0px 0px;
 	border-radius: 5px 5px 0px 0px;
	color: #fff;
	font-family: Tahoma, Geneva, sans-serif;
	font-size: 14px;
}

.box_blue_header h3 {
	font-size: 20px;
	margin: 0px;
	padding: 0px;
	color: #fff;
	text-transform: uppercase;	
}

.box_blue_header .header p {
	margin: 0px;
	padding: 0px;	
}

.box_blue_header .content {
	padding: 10px;
	font-size: 14px;
	color: #777;
	-moz-border-radius: 0px 0px 5px 5px;
 	border-radius: 0px 0px 5px 5px;
}

.box_blue_header hr, .box_orange_header hr {
	border: none;
	height: 1px;
	background: #d1d1d1;	
}

.box_orange_header .header {
	background: #e97640;
	padding: 12px;
	-moz-border-radius: 5px 5px 0px 0px;
 	border-radius: 5px 5px 0px 0px;
	color: #fff;
	font-family: Tahoma, Geneva, sans-serif;
	font-size: 14px;
}

.box_orange_header .header h3 {
	font-size: 18px;
	font-weight: bold;
	margin: 0px;
	padding: 0px;
	color: #fff;
	text-transform: uppercase;	
}

.box_orange_header .content {
	padding: 10px;	
}

.box_orange_header .text {
	height: auto;
	padding: 5px 10px;
	font-size: 12px;
	margin-bottom: 10px;
	width: 185px;
}

.box_orange_header .submit {
	height: 25px;
	font-size: 12px;
}

.box_orange_header a {
	color: #028afb;	
}

.box_orange_header a:hover {
	text-decoration: underline;
}

.box_navy_header {
	border: 1px solid #c1c1c1;
	-moz-border-radius: 5px;
 	border-radius: 5px;
	margin-top: 25px;
	overflow: hidden;
}

.box_navy_header .header {
	background: #023a4c;	
	color: #fff;
	padding: 12px;
}

.box_navy_header .content {
	background: #fff;	
	padding: 12px;
}

.box_quote_left .header {
	padding-bottom: 25px;
	background-image: url(../images/box_quote_left_header.png);
	background-position: left bottom;
	background-repeat: no-repeat;
}

.box_quote_right .header {
	padding-bottom: 25px;
	background-image: url(../images/box_quote_right_header.png);
	background-position: left bottom;
	background-repeat: no-repeat;	
}

.box_grey_border {
	border: 1px solid #c3c3c3;
	-moz-border-radius: 10px;
 	border-radius: 10px;
	padding: 20px;
	color: #000;
}

.box_navy_border {
	border: 3px solid #003a4c;
	-moz-border-radius: 10px;
 	border-radius: 10px;
	padding: 20px;
	background: #f5f9fd;
}

.box_navy_bg {
	background: #023a4c;
	padding: 30px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}

.float {
	float: left;
	width: auto;
}

input.text, input.input-text {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	color: #7a7a7a;
	height: 35px;
	border: 1px solid #d6d6d6;
	padding: 0px 15px;
}

input.size3 {
	width: 25px
}

input.size4 {
	width: 	35px;
}

.system_note {
	border: 1px solid #d1d1d1;
	-moz-border-radius: 6px;
	border-radius: 6px;
	background: #fff url(../images/icon_exclamation_orange.png) 15px 20px no-repeat;
	padding: 15px 15px 15px 50px;
	font-weight: bold;
	color: #044f67;
}

.system_note p {
	margin: 0;
	padding: 0;
}

/* -- header -- */

#header_wrapper{
	overflow: visible;
}

#header {
	
}

#logo {
	display: block;
	width: 305px;
	height: 85px;
	float: left;
	background: url(../images/logo.png) left top no-repeat;	
}

#social {
	background: #f1f1f1;
	width: auto;
	height: 26px;
	padding: 4px 10px;
	float: right;
	overflow: hidden;
	-moz-border-radius: 0px 0px 10px 10px;
 	border-radius: 0px 0px 10px 10px;
	text-align: center;
	margin-bottom: 14px;
}

#header .phone {
	clear: right;
	width: auto;
	float: right;
	font-size: 27px;
}

#user_nav {
	float: right;
	width: auto;
	margin-right: 20px;
	position: relative;
	z-index: 100;
}

#user_nav .user_nav_box {
	width: auto;
	float: left;
	background: url(../images/arrow_down_orange.png) 10px 15px no-repeat;
	border-top: 1px solid #fff;
	padding-right: 2px;
	position: relative;
	z-index: 20;
}

#user_nav a.header {
	font-size: 12px;
	color: #646262;
	display: block;
	width: auto;
	padding: 10px 10px 10px 40px;
}

#user_nav_search .header {
	background: url(../images/icon_search.png) 22px center no-repeat;
}

#user_nav_account .header {
	background: url(../images/icon_account.png) 22px center no-repeat;
}

#user_nav_cart a.header {
	background: url(../images/icon_cart.png) 22px center no-repeat;
	padding-left: 45px;
}

#user_nav .user_nav_content_box {
	position: absolute;
	z-index: 10;
	visibility: hidden;
	background: #f2f2f2;
	border: 1px solid #c1c1c1;
	-moz-border-radius: 0 5px 5px 5px;
	border-radius: 0 5px 5px 5px;
	-webkit-box-shadow: 5px 5px 5px 1px #a4a4a4;
	-moz-box-shadow: 5px 5px 5px 1px #a4a4a4;
	box-shadow: 5px 5px 5px 1px #a4a4a4;
	padding: 30px 30px 25px 50px;
	margin-top: -4px;
	margin-left: -1px;
}

#user_nav .user_nav_box:hover {
	background: #f2f2f2 url(../images/arrow_down_orange.png) 10px 15px no-repeat;
	border: 1px solid #c1c1c1;
	-moz-border-radius: 5px;
	border-radius: 5px;
	-webkit-box-shadow: 5px 5px 5px 1px #a4a4a4;
	-moz-box-shadow: 5px 5px 5px 1px #a4a4a4;
	box-shadow: 5px 5px 5px 1px #a4a4a4;
	padding-right: 1px;
}

#user_nav .user_nav_box:hover .bottom_void {
	background: #f2f2f2;
	height: 2px;
	position: relative;
	z-index: 30;
	width: inherit;
	margin-bottom: -2px;
	top: -5px;
}

#user_nav .user_nav_box:hover .user_nav_content_box {
	visibility: visible;
}

#user_nav_search_box {
	width: 250px;
}

#user_nav_search_box .text {
	width: 100px;
	float: left;
	margin-right: 10px;
	height: 30px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}

.user_nav_content_box .submit {
	float: left;
	height: 32px;
}

#user_nav_account_box {
	width: 270px;
}

#user_nav_account_box .text {
	height: 30px;
	width: 150px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}

#user_nav_account_box #confirm_code {
	width: 108px;
}

#top_menu {
	width: auto;
	height: 40px;
	overflow: hidden;
	float: left;
	background: url(../images/menu_top_bg.png) left center repeat-x;	
	-moz-border-radius: 10px;
 	border-radius: 10px;
}

#top_menu ul {
	margin: 0px;
	padding: 0px 10px;
	list-style: none;
}

#top_menu li {
	display: block;
	width: auto;
	float: left;
	list-style: none;
	padding-right: 2px;
	background: url(../images/menu_top_sep.png) right center no-repeat;
}

#top_menu li.last {
	background: none;
}

#top_menu a {
	display: block;
	width: auto;
	float: left;
	font-family: Tahoma, Geneva, sans-serif;
	color: #fff;
	font-weight: bold;
	height: 20px;
	padding: 10px 20px;
}

#top_menu li:hover a {
	background: url(../images/menu_top_bg_hover.png) left top repeat-x;	
}

#top_menu .top_submenu {
	position: absolute;
	margin-top: 41px;
	z-index: 100;
	background: #fff;
	border: 2px solid #023a4c;
	-moz-border-radius: 0 10px 10px 10px;
	border-radius: 0 10px 10px 10px;
	visibility: hidden;
	padding: 15px;
}

#top_menu li:hover .top_submenu {
	visibility: visible;	
}

#top_menu .top_submenu label {
	display: block;
	padding-bottom: 5px;
}

#top_menu .top_submenu .text {
	width: 60px;
	float: left;
	height: 32px;
	margin-right: 10px;
}

#top_menu .top_submenu .submit {
	float: left;
}

#order_now {
	float: right;
}

/* -- banner -- */

#banner {
	height: 370px;
	overflow: hidden;
	padding: 0px 15px 0px 280px;
	background: url(../images/banner_bg.jpg) left bottom no-repeat;
	font-size: 14px;
}

#banner h2 {
	margin: 0px;
	padding: 0px;
}

#banner h3 {
	margin: 0px;
	padding: 5px 0px 15px 0px;	
}

#banner p {
	line-height: 1.4em;	
}

#prices {
	float: left;
	margin-right: 20px;
}

div.quote {
	font-size: 11px;
	line-height: 1.5em;
	padding-top: 5px;
}

span.quote {
	font-style: italic;
}

#stars {
	width: 230px;
	height: 67px;
	float: left;
	font-weight: bold;
	background: url(../images/reviews_stars.png) left top no-repeat;
}

#stars p {
	padding: 0px 0px 0px 5px;
	margin: 0px;	
}

#trusted { 
	padding-left: 30px;	
	overflow: hidden;
}

#trusted p {
	font-weight: bold;
	margin: 0px;
	padding: 0px 0px 10px 0px	
}

/* -- frontpage --- */

#locations_steps {
	padding: 20px 0px;
	overflow: hidden;
}

#find_location {
	width: 300px;
	float: left;
	overflow: hidden;
}

#find_location p {
	font-size: 14px;
	font-weight: bold;
	margin: 0px;
	padding: 0px;
}

#find_location form {
	padding-top: 10px;	
}

#find_location form .text {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	color: #7a7a7a;
	width: 60px;
	height: 35px;
	border: 1px solid #d6d6d6;
	padding: 0px 15px;
}

#find_location form .submit {
	-moz-border-radius: 10px;
 	border-radius: 10px;
	font-size: 15px;
	font-weight: bold;
	color: #fff;
	text-transform: uppercase;
	width: 156px;
	height: 30px;
	text-align: center;
	border: none;
	background: url(../images/btn_blue_reflection.png) center center no-repeat;
	margin-left: 10px;
}

#locations_steps #steps {
	height: 60px;
	width: 650px;
	float: right;
	background: url(../images/frontpage_steps.png) center center no-repeat;	
}

#benefits {
	width: 700px;
	float: left;
	padding: 10px 0px 20px 0px;
}

#benefits td {
	vertical-align: top;
	padding-right: 25px;
}

#benefits h4 {
	padding: 0px;
	margin: 0px;
}

#recommender {
	float: right;
}

#recommender .content {
	background: url(../images/recommender_box_bg.jpg) right bottom no-repeat;
	padding: 15px 100px 60px 15px;
}

#recommender .btn_blue_reflection {
	width: 80px;
}

/* --- subpages --- */

/* breadcrumbs */

#breadcrumbs a {
	font-size: 10px;
	color: #646262;
	text-decoration: underline;	
}

#breadcrumbs a.current {
	text-decoration: none;
}

#breadcrumbs a:hover {
	color: #000;
}

#breadcrumbs span.separator {
	font-size: 8px;
}	

/* content */

#content {
	padding-bottom: 20px;
	float: left;
	width: 100%;
}

#content #left_col {
	width: 710px;
	float: left;
}

#content #right_col {
	width: 240px;
	float: right;
}

#content .page-title {
	font-size: 28px;
	font-family: Tahoma, Geneva, sans-serif;
	color: #023a4c;
	margin-bottom: 0px;
	padding-bottom: 0px;
}

#content h3 {
	font-size: 19px;
	margin: 0px;
	padding: 5px 0px 10px 0px;
}

/* steps module */

#steps_module {
	border: 1px solid #d1d1d1;
	-moz-border-radius: 15px;
 	border-radius: 15px;
	padding: 10px 15px;
	margin: 5px 0px 15px 0px;
}

#steps_module .steps {
	width: 460px;
	float: left;
	font-size: 11px;
}

#steps_module .steps td {
	padding: 10px;	
}

#steps_module .steps h3 {
	font-size: 24px;
	font-weight: bold;
	padding: 0px;
	margin: 0px;	
}

#steps_module .steps .row_1 h3 {
	color: #11b2e7;
}

#steps_module .steps .row_2 h3 {
	color: #109cca;
}

#steps_module .steps .row_3 h3 {
	color: #0e82a8;
}

#steps_module .steps p {
	margin: 0px;
	padding: 0px;	
}

#steps_module .promo {
	width: 430px;
	float: right;	
}

#steps_module .order {
	width: 275px;
	float: left;
	background: url(../images/btn_arrows_orange.png) center center no-repeat;	
}

#steps_module .order a {
	width: 140px;
	margin: 0px auto;	
}

#steps_module .links {
	width: 140px;
	float: left;
	padding: 8px 0px 0px 15px;
}

#steps_module .links ul {
	margin: 0px;
	padding: 0px;
	list-style: none;	
}

#steps_module .links li {
	display: block;
	margin: 0px;
	padding: 0px 0px 0px 12px;
	list-style: none;	
	background: url(../images/arrow_right_blue.png) left center no-repeat;
}

#steps_module .links a {
	font-weight: bold;
	font-size: 12px;
	color: #028afb;
}

/* location module */

#location_module form {
	margin: 10px 0px;
	padding: 0px 10px;
}

#location_module label {
	font-family: Tahoma, Geneva, sans-serif;
	font-size: 19px;
	color: #0c6e8e;
	padding-right: 5px;	
}

#location_module input.text {
	padding: 10px;
	width: 100px;
	margin: 0px 10px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	color: #7a7a7a;
}

#location_module .submit {
	height: 32px;
	border: none;
	padding: 7px 10px;
	display: inline;
	margin-left: 15px;
}

#location_results .item {
	border-bottom: 1px solid #e3e3e3;
	padding: 10px 0px;
	overflow: hidden;	
}

#location_results .pin {
	width: 50px;
	height: inherit;
	padding-top: 10px;
	float: left;
}

#location_results h4 {
	font-family: Arial, Helvetica, sans-serif;
	color: #000;
	font-size: 16px;
	margin: 0px;
	padding: 0px 0px 15px 0px;
}

#location_results .location {
	float: left;
	width: 170px;
	padding-right: 10px;
}

#location_results p {
	padding: 0px;
	margin: 0px;	
}

#location_results .header {
	font-weight: bold;
	padding-bottom: 5px;
}

#location_results .hours {
    float: left;
    padding-bottom: 10px;
    padding-right: 15px;
    width: 290px;
}

#location_results .btn_orange_25_reflection {
	float: right;
	padding: 6px 6px;
}

/* blog */

#blog_entry .blog_info {
	font-size: 10px;
	color: #646262;	
}

.postWrapper  {
	border-bottom: 1px solid #d3cccc;
	padding-top: 15px;
}

.postWrapper .postTitle {
	border: none;
}

.postWrapper .postTitle h3 {
	padding-bottom: 0;
	margin-bottom: 0;
	font-size: 19px;
	font-weight: bold;
	color: #ea7640;
}

.postWrapper .postTitle .info {
	text-transform: uppercase;
	padding-top: 0;
	margin-top: 0;	
}

.postWrapper .postContent a {
	color: #028afb;
}

/* social links */

ul.socials {
	list-style: none;
	margin-left: 0px;
	padding-left: 0px;
	width: 100%;
	padding-bottom: 25px;
}

ul.socials li {
	display: block;
	width: auto;
	float: left;
	padding-left: 20px;	
	padding-right: 10px;
	list-style: none;
}

ul.socials li.shr-facebook {
